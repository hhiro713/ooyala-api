{
  "name": "ooyala-api",
  "version": "0.11.1",
  "description": "Ooyala API client for Node.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:kuu/ooyala-api.git"
  },
  "keywords": [
    "Ooyala"
  ],
  "main": "./lib.js",
  "bin": {
    "oo": "./cli.js"
  },
  "author": "Kuu Miyazaki",
  "license": "MIT",
  "dependencies": {
    "config": "^1.26.1",
    "debug": "^2.6.8",
    "minimist": "^1.2.0",
    "node-fetch": "^1.7.1",
    "through2-parallel": "^0.1.3"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "coveralls": "^2.13.1",
    "nsp": "^2.7.0",
    "nyc": "^11.1.0",
    "proxyquire": "^1.8.0",
    "rewire": "^2.5.2",
    "sinon": "^2.4.1",
    "xo": "^0.18.2"
  },
  "scripts": {
    "oo": "node ./cli.js",
    "lint": "xo",
    "test": "npm run lint && nsp check && ava --verbose",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "camelcase": 0,
      "capitalized-comments": 0,
      "new-cap": 0,
      "no-await-in-loop": 0,
      "no-multi-assign": 0,
      "no-return-assign": 0,
      "import/no-dynamic-require": 0
    }
  }
}
